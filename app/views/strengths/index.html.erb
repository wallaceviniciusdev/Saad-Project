<p id="notice"><%= notice %></p>

<h1>Listing Strengths</h1>

<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>Plano</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @strengths.each do |strength| %>
      <tr>
        <td><%= strength.text %></td>
        <td><%= strength.plano %></td>
        <td><%= link_to 'Show', strength %></td>
        <td><%= link_to 'Edit', edit_strength_path(strength) %></td>
        <td><%= link_to 'Destroy', strength, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Strength', new_strength_path %>
