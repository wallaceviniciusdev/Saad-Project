<link href="http://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link rel="stylesheet" href=https://daneden.github.io/animate.css/animate.min.css">

<div class="background"></div>
<div class="navegacao">
  <div class="container">
    <nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Início</a></li>
    <li class="breadcrumb-item">Competências Socioemocionais</li>
    <li class="breadcrumb-item"><a href="javascript:;"><b>Listar Resultados de Competências socioemocionais</b></a></li>
  </ol>
</nav>
  </div>
</div>

<div class="container">
<div class="panel panel-default">
  <div class="panel-heading">
    <h4>Listar Resultados de Competências socioemocionais
    <a href="javascript:history.back()"><h4><span class="badge badge-warning"><i class="fas fa-arrow-left"></i> Voltar</span></h4></a></h4>
  </div>
      <div class="row">
          <div class="col-lg-12">
          <div class="panel-body">

			<%= javascript_include_tag "results" %>
			<% if current_user.type == 'Teacher' || current_user.type == 'Coordinator'%>
			    <%= render :partial => 'result_socio_emotionals/teacher_search' %>
			<% elsif current_user.type == 'Principal' %>
			    <%= render :partial => 'result_socio_emotionals/principal_search' %>
			<% end %>

		  </div>
		  </div>
		 </div>

</div>
</div>

<script>
	var nome_usuario = "<%= current_user.nome.downcase.titleize %>";
	var novo_nome = nome_usuario.split(" ");

	$(document).ready(function() {
	    document.title = 'Listar Resultados de Competências socioemocionais - ' + novo_nome[0] + " " + novo_nome[novo_nome.length-1] + " - SAAD";
	});

</script>


