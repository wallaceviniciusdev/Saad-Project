       
        <%= form_tag(show_by_date_learning_results_path, method: 'post', :remote => true) do %>
              <%= hidden_field_tag "selecao", @selecao %>
              <div class="alert alert-primary" role="alert">
                Selecione uma data para vizualizar o resultado 
                <%= select_tag(:data_final, options_for_select(@datas),:class => "form-control") %>
              </div>
              <% @results.map(&:id).each do |id| %>
                <%= hidden_field_tag "results[]", id %>
              <% end %>
              </br>
                      <%= hidden_field_tag "allUsers", @allUsers %>
              <center><%= submit_tag "Visualizar Estilo ", class: "btn btn-login"%></center>
              <% end %>

              <div id="chartjs" class="row mt">
                  <div class="col-lg-6" class="table table-bordered table-striped table-condensed">
                        <div id="graphic"></div>
                  </div>
                  <div class="col-lg-6">
                        <div id="graphic2"></div>
                  </div>


              </div>
