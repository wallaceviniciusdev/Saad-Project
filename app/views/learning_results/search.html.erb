      <div class="row">
          <div class="col-lg-12">
          <div class="panel-body">
			<% if current_user.type == 'Teacher'  || current_user.type == 'Coordinator'%>
			    <%= render :partial => 'learning_results/teacher_search' %>
			<% elsif current_user.type == 'Principal' %>
			    <%= render :partial => 'learning_results/principal_search' %>
			<% end %>
		  </div>
		  </div>
 		</div>
  </div>
</div>

<script>
  var nome_usuario = "<%= current_user.nome.downcase.titleize %>";
  var novo_nome = nome_usuario.split(" ");

  $(document).ready(function() {
      document.title = 'Listar Resultados de Estilos de Aprendizagem - ' + novo_nome[0] + " " + novo_nome[novo_nome.length-1] + " - SAAD";
  });

  
</script>


